<div class="container">
  <h1>Add an Owner</h1>
  <%= form_for @person do |f| %>
    <fieldset>
      <legend>Add Person:</legend>
      <%=f.label :name %>
      <%=f.text_field :name %>
      <br><br>
      <legend>Add the url of a photo</legend>
      <%=f.label :photo  %>
      <%=f.text_field :photo %>
    </fieldset>
    <br>
    <%= f.submit("Add") %>
  <% end %>
</div>
